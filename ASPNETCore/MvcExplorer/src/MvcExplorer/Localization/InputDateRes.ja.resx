    <value>前の週の数</value>
    <comment>WeekBefore</comment>
  </data>
  <data name="WeekAfter_Text0" xml:space="preserve">
    <value>後ろの週の数</value>
    <comment>WeekAfter</comment>
  </data>
  <data name="MonthCount_Text0" xml:space="preserve">
    <value>月数</value>
    <comment>MonthCount</comment>
  </data>
