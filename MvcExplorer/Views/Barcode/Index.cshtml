    </tbody>
</table>
      
@section Scripts{
    <script>
        function OnSelectedItemChanged(e, s) {
            var selectedItem = e.selectedItem;
            if (!selectedItem.Items) {
                $(".barcode-control-result").css("opacity", 0);
                $("#" + selectedItem.Name).css("opacity", 1);
            }
        }

        function OnLoadedItems(e) {
            e.getFirstNode().select();
        }
    </script>
} 

@section Summary{
    <p>@Html.Raw(Resources.Barcode.Barcode_Description_Text0)</p>
}

