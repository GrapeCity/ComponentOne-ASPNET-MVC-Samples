@model object[]

<link href="~/Content/css/barcode.css" rel="stylesheet" />

<table class="table">
    <thead>
        <tr>
            <th>@Html.Raw(Resources.Barcode.Barcode_Index_Text0)</th>
            <th>@Html.Raw(Resources.Barcode.Barcode_Index_Text2)</th>
        </tr>
    </thead>
    <tbody>
