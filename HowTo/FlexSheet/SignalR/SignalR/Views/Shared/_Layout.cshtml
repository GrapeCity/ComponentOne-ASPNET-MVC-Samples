<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>SignalR - ComponentOne ASP.NET MVC FlexSheet</title>
    <link rel="apple-touch-icon" sizes="180x180" href="@Url.Content("~/favicon/apple-touch-icon.png")">
    <link rel="icon" type="image/png" href="@Url.Content("~/favicon/favicon-32x32.png")" sizes="32x32">
    <link rel="icon" type="image/png" href="@Url.Content("~/favicon/favicon-16x16.png")" sizes="16x16">
    <link rel="manifest" href="@Url.Content("~/favicon/manifest.json")">
    <link rel="mask-icon" href="@Url.Content("~/favicon/safari-pinned-tab.svg")" color="#503b75">



    @Html.C1().Styles().Theme(Themes.Office)
    @Styles.Render("~/Content/bootstrap.css")
    @Styles.Render("~/Content/css")
    @RenderSection("styles", required: false)

    @Html.C1().Scripts().Basic(b => b.Input()).FlexSheet()
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/signalr")
    @RenderSection("scripts", required: false)
</head>
<body>


    <!-- page header -->
    <div class="header">
        <div class="container">
            <a class="logo-container" href="https://www.grapecity.com/en/aspnet-mvc" target="_blank">
                <i class="gcicon-product-c1"></i>
            </a>
            <h1>
                FlexSheet SignalR
            </h1>
            <p>
                Use SignalR with FlexSheet for ASP.NET MVC.
            </p>
        </div>
    </div>
    @RenderBody()
</body>
</html>
